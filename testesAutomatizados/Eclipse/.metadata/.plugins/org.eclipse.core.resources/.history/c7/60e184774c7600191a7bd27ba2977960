import org.openqa.selenium.firefox.FirefoxDriver;


public class Google {
	
	FirefoxDriver pagina;
	
    @Before
    public void antes() {
    	System.setProperty("webdriver.gecko.driver", "C:\\Users\\181710062\\Desktop\\Automatizados\\eclipse\\geckodriver.exe");
    	pagina = new FirefoxDriver();
    	pagina.get("https://www.google.com.br/?cc=br");
    	
    }
	@Test
